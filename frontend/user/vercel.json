{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist/user/browser" }
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|webp|woff2?|ttf|eot|json))",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "continue": true
    },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
